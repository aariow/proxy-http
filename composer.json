{
  "name": "aariow/proxy-http",
  "description": "Simple HTTP proxy (URL redirector) built with PHP",
  "type": "project",
  "keywords": ["php", "proxy", "http"],
  "license": "MIT",
  "require": {
    "php": "^8.1.2",
    "symfony/http-foundation": "^6.1",
    "symfony/var-dumper": "^6.1",
    "psr/container": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "helpers/common.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
